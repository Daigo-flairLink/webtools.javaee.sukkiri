            class=&quot;org.eclipse.jst.j2ee.xdoclet.runtime.internal.tasks.UtilObjectEjbdocletTaskProvider&quot;
            defaultSelection=&quot;true&quot;
            description=&quot;%ejbdoclet.UtilObject.desc&quot;
