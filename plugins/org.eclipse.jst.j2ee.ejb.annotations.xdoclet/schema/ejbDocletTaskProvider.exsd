      <documentation>
           &lt;extension
         id=&quot;org.eclipse.jst.j2ee.xdoclet.ejbDocletTaskProvider.utilobject&quot;
         name=&quot;%ejbdoclet.UtilObject&quot;
         point=&quot;org.eclipse.jst.j2ee.ejb.annotations.xdoclet.ejbDocletTaskProvider&quot;&gt;
